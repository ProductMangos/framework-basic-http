    <AssemblyName>BasicHttpServiceSample</AssemblyName>
    <NoStandardLibraries>false</NoStandardLibraries>
    <OutputType>Exe</OutputType>
    <RootNamespace>Microsoft.Samples.BasicHttpService</RootNamespace>
    <TargetFramework>net8.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <None Include="App.config"></None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.ServiceModel.Http" Version="8.1.2"></PackageReference>
    <PackageReference Include="System.ServiceModel.Primitives" Version="8.1.2"></PackageReference>
    <PackageReference Include="CoreWCF.Http" Version="1.8.0"></PackageReference>
    <PackageReference Include="CoreWCF.Primitives" Version="1.8.0"></PackageReference>
    <PackageReference Include="CoreWCF.WebHttp" Version="1.8.0"></PackageReference>
  </ItemGroup>
  <ProjectExtensions>
    <VisualStudio AllowExistingFolder="true"></VisualStudio>
  </ProjectExtensions>
</Project>